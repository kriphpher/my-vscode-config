{
    "breadcrumbs.enabled": false,
    "css.lint.emptyRules": "ignore",
    "editor.fontFamily": "JetBrains Mono",
    "editor.rulers": [],
    "editor.minimap.enabled": false,
    "editor.renderLineHighlight": "gutter",
    "editor.fontLigatures": true,
    "editor.semanticHighlighting.enabled": false,
    "editor.scrollbar.horizontal": "hidden",
    "editor.scrollbar.vertical": "hidden",
    "editor.stickyScroll.enabled": false,
    "editor.tabSize": 2,
    "explorer.compactFolders": false,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.patterns": {
        "*.ts": "${capture}.js",
        "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
        "*.jsx": "${capture}.js",
        "*.tsx": "${capture}.ts",
        "tsconfig.json": "tsconfig.*.json",
        "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb"
    },
    "extensions.autoUpdate": true,
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 100,
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "git.openRepositoryInParentFolders": "never",
    "livePreview.autoRefreshPreview": "On Changes to Saved Files",
    "livePreview.serverRoot": "-",
    "outline.problems.badges": false,
    "outline.problems.colors": false,
    "prettier.enable": true,
    "prettier.enableDebugLogs": true,
    "prettier.arrowParens": "always",
    "problems.autoReveal": false,
    "problems.decorations.enabled": false,
    "problems.showCurrentInStatus": false,
    "security.workspace.trust.untrustedFiles": "open",
    "settingsSync.ignoredExtensions": [
        "catppuccin.catppuccin-vsc"
    ],
    "settingsSync.ignoredSettings": [
        "editor.tabSize"
    ],
    "symbols.hidesExplorerArrows": false,
    "task.problemMatchers.neverPrompt": false,
    "workbench.layoutControl.enabled": false,
    "workbench.activityBar.location": "side",
    "workbench.statusBar.visible": false,
    "workbench.editor.labelFormat": "short",
    "workbench.startupEditor": "newUntitledFile",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorCustomizations": {
        "activityBar.background": "#000",
        "activityBar.border": "#000",
        "editor.background": "#000",
        "editorError.foreground": "#4e4e4e",
        "editorGroupHeader.tabsBackground": "#000",
        "focusBorder": "#000",
        "menu.background": "#000",
        "sideBar.background": "#000",
        "sideBar.border": "#000",
        "sideBar.foreground": "#4e4e4e",
        "sideBar.dropBackground": "#4e4e4e",
        "statusBar.background": "#000",
        "statusBar.border": "#000",
        "selection.background": "#000",
        "menu.border": "#000",
        "tab.border": "#000",
        "input.border": "#000",
        "merge.border": "#000",
        "scrollbarSlider.activeBackground": "#000",
        "tab.activeBackground": "#000",
        "tab.inactiveBackground": "#000",
        "tab.activeBorder": "#000",
        "tab.activeBorderTop": "#000",
        "titleBar.activeBackground": "#000",
        "welcomePage.background": "#000",
        "editorCursor.background": "#4e4e4e",
        "editor.selectionHighlightBackground": "#000",
        "editor.lineHighlightBackground": "#000",
        "editor.lineHighlightBorder": "#000",
        "editorLineNumber.activeForeground": "#4e4e4e",
        "editorLineNumber.foreground": "#4e4e4e",
        "editorLineNumber.dimmedForeground": "#000",
        "editorGutter.background": "#000",
    },
    "workbench.editorAssociations": {
        "*.otf": "default"
    },
    "workbench.editor.enablePreview": false,
    "window.menuBarVisibility": "compact",
    "workbench.colorTheme": "Firefox Dark",
    "window.zoomLevel": 0,
    "window.commandCenter": false,
    "apc.electron": {
        "titleBarStyle": "hiddenInset",
        "frame": false,
        "trafficLightPossition": {
            "x": 11,
            "y": 10,
        },
    },
    "apc.header": {
        "height": 36,
    },
    "apc.font.family": "Inter",
    "apc.listRow": {
        "height": 24,
    },
    "apc.stylesheet": {
        ".title-label > h2": "display: none;",
        ".editor-actions": "display: none;",
        ".pane-body": "padding: 8px;",
        ".pane-header": "padding: 0 8px;",
        ".split-view-view:first-child .pane-header": "display: none !important;",
        ".monaco-list-row": "border-radius: 6px;",
        ".monaco-workbench .monaco-list:not(.element-focused):focus:before": "display: none;",
    },
}
